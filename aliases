alias cat="bat -p"
alias ga="git commit --amend --no-edit"
alias gcm="git checkout main; git checkout master; git pull; git pull upstream main; git pull upstream master;"
alias gd="git diff"
alias gs="git status"
alias purge="curl -X PURGE"
alias update="brew update && brew upgrade && brew cleanup -s"
alias wipe="git reset --hard && git clean -df"
